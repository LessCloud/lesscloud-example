{
  "name": "lesscloud-web-example",
  "version": "0.0.1",
  "description": "This web example is used to demonstrate for LessCloud App",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lesscloud/web-example"
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true
  },
  "scripts": {
    "app-builder": "set process.env.NODE_ENV=development && node ./build/app-builder.js",
    "build": "set process.env.NODE_ENV=production && webpack --config build/webpack.client.config.js --progress --colors",
    "dev": "cross-env NODE_ENV=development webpack --config build/webpack.client.config.js --progress --colors --watch --hide-modules",
    "start": "webpack-dev-server --config build/webpack.client.config.js --history-api-fallback --progress --colors"
  },
  "keywords": [
    "LessCloud",
    "js",
    "SDK"
  ],
  "author": "Stephen Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lesscloud/web-example/issues"
  },
  "homepage": "https://github.com/lesscloud/web-example",
  "private": false,
  "dependencies": {
    "bootstrap": "3.3.7",
    "compression": "^1.6.2",
    "element-ui": "^1.2.7",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "lesscloud-sdk": "^0.0.6",
    "lru-cache": "^4.0.2",
    "serve-favicon": "^2.4.1",
    "vue": "^2.2.4",
    "vue-router": "^2.3.0",
    "vue-style-loader": "^2.0.4",
    "vue-validator": "^2.1.7",
    "vuex": "^2.2.1",
    "vuex-router-sync": "^4.1.2",
    "webpack-merge": "^4.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.0.0",
    "babel-plugin-component": "^0.9.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.13.2",
    "babel-runtime": "^5.8.34",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "buble": "^0.15.2",
    "buble-loader": "^0.4.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "express": "^4.15.2",
    "file-loader": "^0.10.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.3.1",
    "less-loader": "^4.0.3",
    "lesscloud-app-builder": "^0.0.5",
    "rimraf": "^2.6.1",
    "prompt": "^1.0.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.9.1",
    "url-loader": "^0.5.8",
    "vue-hot-reload-api": "^1.2.0",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.4",
    "vueify": "^8.0.0",
    "watchify": "^3.6.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.17.1"
  }
